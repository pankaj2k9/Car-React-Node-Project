{
  "name": "wildebeest",
  "version": "1.7.16",
  "description": "A simple Node.js app using Express 4 with Webpack, React, alt, ImmutableJS",
  "repository": "https://github.com/wildebeestdev/wb-react-boilerplate",
  "main": "index.js",
  "scripts": {
    "babel": "babel --ignore node_modules,public --presets @babel/env",
    "babel-node": "babel-node --ignore node_modules,public --presets @babel/env",
    "lint": "eslint app server",
    "clean": "rimraf public && rimraf compiled",
    "start": "cross-env NODE_ENV=production node compiled/index.js",
    "dev": "yarn install && npm run clean && cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development --config ./webpack/webpack.dev.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production --config ./webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:node": "cross-env npm run babel -- ./server -d ./compiled",
    "build": "npm run clean && npm run build:prod && npm run build:node"
  },
  "author": "Wildebeest Design & Development",
  "license": "MIT",
